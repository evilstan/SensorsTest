<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.evilstan.sensorstest">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.SensorsTest">
    <activity
      android:exported="false"
      android:name=".TestFlash" />
    <activity
      android:exported="false"
      android:label="@string/title_activity_test2"
      android:name=".TestActivity2"
      android:theme="@style/Theme.SensorsTest.NoActionBar" />
    <activity
      android:exported="false"
      android:name=".TestVibrator" />
    <activity
      android:exported="true"
      android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  </application>

  <uses-permission android:name="android.permission.VIBRATE" />

</manifest>